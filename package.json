    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.5",
    "lucide-react": "^0.546.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4",
    "secretlint": "^11.2.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.15"
  },
